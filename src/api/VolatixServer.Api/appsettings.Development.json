{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Cors": {
    "PolicyName": "AllowAngularApp",
    "AllowedOrigins": [
      "http://localhost:4200",
      "http://localhost:4201",
      "http://localhost:4202",
      "https://localhost:4200",
      "https://localhost:4201",
      "https://localhost:4202"
    ],
    "AllowAnyMethod": true,
    "AllowAnyHeader": true,
    "AllowCredentials": true,
    "AllowWildcardSubdomains": true,
    "ExposeAllHeaders": true
  }
}
